    "@types/node": "^13.7.0",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tsify": "^4.0.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
