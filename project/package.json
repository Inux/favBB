{
  "name": "weblab-project",
  "version": "1.0.0",
  "description": "Project for the HSLU Weblab module!",
  "main": "index.js",
  "scripts": {
    "compile": "browserify --debug src/main.ts -p [ tsify --project src/tsconfig.json ] -o public/js/app.js",
    "clean": "rm -f public/js/app.js",
    "start": "budo src/main.ts:js/app.js -p 3000 --dir public --live=*.{html,js,css} -- --debug -p [ tsify --project src/tsconfig.json ]"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mithril": "^2.0.4"
  },
  "devDependencies": {
    "@types/mithril": "^2.0.2",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "npm-run-all": "^4.1.5",
    "tsify": "^4.0.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "uglify-js": "^3.7.7"
  }
}
